{
  "name": "integration_tmdb",
  "version": "0.0.1",
  "description": "TMDB integration",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.js",
    "dev": "NODE_ENV=development webpack --progress --config webpack.js",
    "watch": "NODE_ENV=development webpack --progress --watch --config webpack.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "stylelint": "stylelint src/**/*.vue src/**/*.scss src/**/*.css",
    "stylelint:fix": "stylelint src/**/*.vue src/**/*.scss src/**/*.css --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-nc/integration_tmdb"
  },
  "keywords": [
    "tmdb",
    "TMDB"
  ],
  "author": "Julien Veyssier",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/julien-nc/integration_tmdb/issues"
  },
  "homepage": "https://github.com/julien-nc/integration_tmdb",
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "engines": {
    "node": "^24.0.0",
    "npm": "^11.3.0"
  },
  "dependencies": {
    "@nextcloud/auth": "^2.2.1",
    "@nextcloud/axios": "^2.4.0",
    "@nextcloud/dialogs": "^7.0.0-rc.1",
